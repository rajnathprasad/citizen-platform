<h2>Edit Scheme</h2>

<form action="/admin/schemes/edit/<%= scheme._id %>" method="POST">
    <div>
        <label for="title">Title</label>
        <input type="text" id="title" name="title" value="<%= scheme.title %>" required>
    </div>

    <div>
        <label for="description">Description</label>
        <textarea id="description" name="description" required><%= scheme.description %></textarea>
    </div>

    <button type="submit">Update Scheme</button>
</form>
